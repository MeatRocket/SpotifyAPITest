@page "/"

<PageTitle>Index</PageTitle>

<h1>Hello, world!</h1>

Welcome to your new app.

<SurveyPrompt Title="How is Blazor working for you?" />

<button @onclick=OnButtonClick>IDK</button>

@s


<button @onclick=CallHttp>Toggle Play</button>

@if (Artists != null)
{
    foreach (FullArtist artist in Artists)
    {
        <div>@artist.Name</div>
    }
}

@if (Tracks != null)
{
    foreach (SavedTrack track in Tracks)
    {
        <div>@track.Track.Name</div>
    }
}